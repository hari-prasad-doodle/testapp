{
    "env": {
        "browser":true,
        "node": true,
        "es2020": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars":1,
        "space-infix-ops":2,
        "comma-spacing":1,
        "brace-style":1,
        "curly":2,
        "handle-callback-err":2,
        "no-undef":2,
        "operator-linebreak":2,
        "no-cond-assign":2,
        "block-spacing":1,
        "camelcase":2,
        "comma-dangle":2,
        "comma-style":1,
"dot-location":0,
"func-call-spacing":2,
"key-spacing":1,
"new-cap":2,
"constructor-super":2,
"valid-typeof":2,
"template-curly-spacing":2,
"space-unary-ops":2,
"space-in-parens":2,
"semi-spacing":1,
"rest-spread-spacing":2,
"padded-blocks":"off",
"object-property-newline":2,
"no-useless-rename":2,
"no-useless-constructor":2,
"no-useless-computed-key":2,
"no-useless-call":2,
"no-unsafe-negation":2,
"no-unsafe-finally":2,
"no-unneeded-ternary":2,
"no-undef-init":2,
"no-this-before-super":2,
"no-new-require":2,
"no-lone-blocks":2,
"no-const-assign":2,
"no-class-assign":2,
"no-extra-parens":2,
"no-extra-boolean-cast":2,
"no-eval":2,
"no-duplicate-case":2,
"no-duplicate-imports":2,
"no-dupe-keys":2,
"no-dupe-args":2,
"no-dupe-class-members":2,
"new-parens":2
    }
}
